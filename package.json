{
  "name": "@slft/ui-kit",
  "version": "0.1.0",
  "description": "Reusable UI components library with PrimeReact, Tailwind CSS and React Hook Form",
  "author": "MrRainx",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles": "./dist/styles/index.css",
    "./styles/variables": "./src/styles/_variables.scss",
    "./theme": {
      "types": "./dist/theme/index.d.ts",
      "import": "./dist/theme/index.mjs",
      "require": "./dist/theme/index.js"
    }
  },
  "files": [
    "dist",
    "src/styles"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:lib": "tsup",
    "start": "next start",
    "lint": "next lint",
    "prepublishOnly": "npm run build:lib",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "clsx": "^2.1.1",
    "primeicons": "^7.0.0",
    "primereact": "^10.9.7",
    "react-datepicker": "^9.1.0",
    "react-hook-form": "^7.71.1",
    "react-number-format": "^5.4.4",
    "react-select": "^5.10.2",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.0",
    "@storybook/addon-a11y": "^10.2.1",
    "@storybook/addon-docs": "^10.2.1",
    "@storybook/addon-onboarding": "^10.2.1",
    "@storybook/addon-vitest": "^10.2.1",
    "@storybook/nextjs-vite": "^10.2.1",
    "@storybook/react": "^10.2.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-datepicker": "^7.0.0",
    "@types/react-dom": "^18",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^8",
    "eslint-config-next": "14.2.35",
    "eslint-plugin-storybook": "^10.2.1",
    "next": "14.2.35",
    "playwright": "^1.58.0",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "sass": "^1.97.3",
    "storybook": "^10.2.1",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.5.1",
    "typescript": "^5",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "react",
    "ui-kit",
    "components",
    "primereact",
    "tailwindcss",
    "design-system"
  ]
}
